@media (min-width: 992px){
    #wrap{
        margin-left: 170px !important;
        float: inherit !important;
        width: inherit !important;
    }
    nav,#logo{
        width: 170px !important;
    }
    #right_header {
        float: inherit;
        width: inherit;
    }
    section div.sec_header {
        height: 50px;
    }
    .li-title {
        margin-top: 3px;
    }
    .active a{
        position: absolute;
        left: 0;
    }
    nav{
      text-align: center;
      padding-top: 4px;
      padding-left: 0px;
      min-height: 320px;  /*logo_min_height-(header_height+footer_height)*/
    }

    nav ul li{
      height: 80px;
      padding-top: 6px;
    }
}
@media (max-width: 767px){
    #foreground{
        left: -142px;
    }
    nav{
        width: 150px !important;
    }
    #logo {
        background-color: #81d869;
    }
    #right_header{
        float: left;
        width: 150px;
        padding-right: 0px;
        font-family: 'helvetica_neueultralight';
        font-size: 11pt;
        color: #e3e4e4;
    }
    #wrap{
        left:8px;
    }
    #logout {
        width: auto;
        right: 32px;
        top: 29px;
        border-left: none;
    }
    #logout-icon {
        font-size: 12pt;
        top: 2px;
        left: 0;
    }
    #logout-text{
        position: relative;
        top: -13px;
        left: 5px;
    }
    #user-icon-circle {
        width: 36px;
        height: 36px;
        top: 50%;
        margin-top: -18px;
        margin-left: 20px;
        border-radius: 18px;
    }
    #user-icon {
        font-size: 25pt;
        top: -14px;
        left: 1px;
    }
    #username {
        position: relative;
        left: 62px;
        font-size: 12pt;
        top: 10px;
        color: #b5e5a7;
    }
    #user_settings {
        height: inherit;
        position: relative;
        width: 150px;
        float: right;
        margin-top: -50px;
    }
    .bg-color{
        background-color: #595858;
    }
    #menu-switch {
        position: absolute;
        color: #e3e4e4;
        left: 40px;
        font-size: 20pt;
    }
    h3 {
        font-size: 26pt;
    }
    .left-offset{
        left:0px !important;
    }
    .main-offset{
        left:150px !important;
    }

}

@media (max-width: 767px){
    .visible-xs{
        display: block;
    }
    .visible-sm, .visible-md, .visible-lg{
        display: none;
    }
}
@media (min-width: 768px){
    .visible-sm{
        display: block;
    }
    .visible-xs, .visible-md, .visible-lg{
        display: none;
    }
}
@media (min-width: 992px){
    .visible-md{
        display: block;
    }
    .visible-xs, .visible-sm, .visible-lg{
        display: none;
    }
}
@media (min-width: 1200px){
    .visible-lg{
        display: block;
    }
    .visible-xs, .visible-sm, .visible-md{
        display: none;
    }
}
