html,
body {
    height: 100%;
    background-color: #e3e4e4;
    font-family: 'helvetica_neuelight';
    color: #797979
}
a {
    color: #797979;
    text-decoration: none;
    cursor:pointer;
    font-size: 12pt;
    float: left;
}
a:hover 
{
    color: #797979;
    text-decoration:none; 
    cursor:pointer;  
}
.arrow {
    width:0; height:0;
    border:10px solid transparent;
    position: relative;
    float: right;
    display: none;
}

.left_arrow {
    border-right:10px solid black;
}
.down_arrow {
    border-top: 8px solid #797979;
    width: 0;
    height: 0;
    border-bottom: 8px solid transparent;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
}
.right_arrow {
    border-left: 10px solid #81d869;
    left: 23px;
    top: 25px;
}
.active a{
    color: #81d869;
}
.active a:hover{
    color:#81d869; 
    text-decoration:none; 
    cursor:pointer;  
}
.active > .arrow{
    display: block;
}
.separator{
    width: 140%;
    border-bottom: solid 1px #797979;
    margin-left: -30px;
    z-index: 1;
}
#wrap {
    min-height: 100%;
    height: auto;
    margin: 0 auto -40px;
    padding: 0 15px 40px 15px;
    overflow: hidden;
    background-color: #e3e4e4;
}

.my_container {
  margin-right: auto;
  margin-left: auto;
  padding-left: 15px;
  padding-right: 15px;
  background-color: #3d3d3d;
}

header {
	background-color: #f5f5f5;
    width: auto;
    height: 70px;
}
h1{
	color: #83e269;
    font-size: 32pt;
    font-family: 'helvetica_neueultralight';
    line-height: initial;
    margin-top: 0;
    margin-bottom: 0;
}
h2{
    color: #797979;
    font-size: 20pt;
    text-shadow: 0px 0px 6px #FFF;
    font-family: 'helvetica_neueultralight';
    line-height: initial;
    margin-top: 0;
    margin-bottom: 0;
}
h3{
    font-family: 'helvetica_neueultralight';
    color: #f5F5F5;
    font-size: 36pt;
    line-height: inherit;
    margin-top: 0;
    margin-bottom: 0;
}
h4{
  font-size: 24px;
  font-family: 'helvetica_neuecondensed_bold';
}

#logo{
	background-color: #3d3d3d;
    height: 100%;
    padding-top: 10px;
    text-align: center;
}

#right_header{
    height: inherit;
    padding-right: 0px;
}
#head_title{
    position: relative;
    float: left;
    padding-top: 13px;
    height: 60px;
}
#user_settings{
    height: inherit;
    position: relative;
    width: 120px;
    float: right;
}
#user-icon-container {
    float: left;
}
#user-icon-circle{
    position: absolute;
    background-color: #9aff7f;
    width: 46px;
    height: 46px;
    top: 50%;
    margin-top: -23px;
    margin-left: 12px;
    border-radius: 23px;
    overflow: hidden;
    border: solid 1px #797979;
}
#user-container-layer {
    width: 100%;
    background-color: #8ce973;
    height: 50%;
}
#user-icon {
    font-size: 30pt;
    color: #FFF;
    position: relative;
    top: -14px;
    left: 3px;
}
#user-down-arrow {
    position: absolute;
    top: 50%;
    left: 94px;
}
#gear-icon {
    font-size: 25pt;
    position: relative;
    top: 50%;
    float: left;
    margin-top: -16.5px;
}
#logout{
	height: inherit;
	position: relative; 
    width: 45px;
    float: right;
    border-left: solid 1px #797979;
    transition: color 0.5s ease;
}
#params{
    height: inherit;
    color: #797979;
    text-decoration: none;
    cursor:pointer;
    font-size: 12pt;
    float: left;
}
#logout:hover{
    color: #81d869;
}
#logout-icon {
    font-size: 20pt;
    position: relative;
    left: 50%;
    top: 30%;
    margin-left: -13.5px;
}
.clear{
	clear: both;
	height: 0px;
    margin: 0px;
}
nav{
	color: #797979;
  position: relative;
  float: left;
  padding-top: 15px;
  padding-left: 15px;
  min-height: 290px;  /*logo_min_height-(header_height+footer_height)*/
  background-color: #3d3d3d;
  margin-bottom: -9999px;
  padding-bottom: 9999px;
}

@media (min-width: 992px) {
    #wrap{

    }
}   
nav ul{
	display: inline;
    list-style:none;
    -webkit-padding-start:0;
}
nav ul li{
	height: 60px;
    margin-top: 10px;
    padding-top: 5px;
}
.li-icon{
    font-size: 38pt;
}
ul > .active{
    color: #81d869;
    border-right: solid 5px #81d869;
    font-size: 38pt;
}
/*nav ul li a {

}*/
#content{
    padding-top: 15px;
    min-height: 290px;  /*logo_min_height-(header_height+footer_height)*/
}

#content a {
    transition: color 0.2s ease;
}

#content a:hover 
{
    color: #81d869;
    text-shadow: 0px 0px 3px #FFF;
}
/*#content section{

}*/
.grid_tile {
  background-color: #fdfdfd;
  padding: 15px;
  margin-bottom: 30px;
  box-shadow: 0px 1px 8px #aaa; 
}
section div.sec_header{
	  height: inherit;
    margin-top: -15px;
    padding-top: 10px;
    padding-left: 15px;
    padding-bottom: 10px;
    padding-right: 15px;
    background-color: #cccccc;
}

#temp_chart {
    height: 400px;
    margin-bottom: 20px;
}
#leftFreq {
  width: 42px;
  padding-right: 0;
  margin-top: 25px;
}
#preview {
  width: inherit;
  height: 90px;
  padding-left: 5px;
  padding-right: 5px;
}
#rightFreq {
  width: 42px;
  padding-left: 0;
  margin-top: 25px;
}
#leftFreq > a, #rightFreq > a {
  font-size: 20pt;
}
section div.sec_content{
    padding: 20px;
}
footer{
	height: 40px;
    background-color: #f5f5f5;
    border-top: solid 1px #797979;
    margin: 0 0 0 170px !important;
    padding: 0 !important;
}
.clearfix:before,
.clearfix:after {
  content: ".";    
  display: block;    
  height: 0;    
  overflow: hidden; 
}
.clearfix:after {clear: both;}
.clearfix {zoom: 1;}

#foreground{
    overflow: hidden;
    position: absolute;
    width: 100%;
    height: 100%;
}

.zindex-content{
    z-index: 1;
}
.zindex-left{
    z-index: 2;
}
.zindex-settings{
    z-index: 2;
}
.zindex-header{
    z-index: 1;
}
#foreground{
    left: 0px;
}
#wrap{
    left:0px;
}

