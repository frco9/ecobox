<% provide(:head_title, "Modification actionneur #{@actuator.name}") %>

<%= form_for @actuator, remote: true, html: {class: "form-horizontal"} do |f| %>
	<%= render 'fields', f: f %>
	<div class="form-group">
    	<div class="col-sm-offset-4 col-sm-8">
			<%= f.submit "Enregistrer", class: "btn btn-large btn-primary" %>
		</div>
	</div>
<% end %>