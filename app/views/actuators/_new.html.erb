<% provide(:head_title, 'Nouvel actionneur') %>

<%= form_for @actuator, remote: true, html: {class: "form-horizontal", novalidate: true} do |f| %>
	<%= render 'fields', f: f %>
	<div class="form-group">
    	<div class="col-sm-offset-4 col-sm-8">
			<%= f.submit "Ajouter un nouvel actionneur", class: "btn btn-large btn-primary" %>
		</div>
	</div>
<% end %>